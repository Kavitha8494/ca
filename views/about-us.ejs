    <script>
      // Due Date Reminder functionality
      (function() {
        const monthLinks = document.querySelectorAll('.month-link');
        const contentContainer = document.getElementById('dueDateReminderContent');
        const marqueeBox = document.getElementById('dueDateReminderMarquee');
        let currentMonth = 'all';

        // NOTE: For BLOGS we will render stored HTML as-is (including <a> tags),
        // so we don't need to strip HTML here.

        // Function to highlight selected month
        function highlightMonth(selectedMonth) {
          monthLinks.forEach(link => {
            link.classList.remove('active-month');
            if (link.getAttribute('data-month') === selectedMonth) {
              link.classList.add('active-month');
            }
          });
        }

        // Function to load and display due date reminders
        async function loadDueDateReminders(month = 'all') {
          try {
            const response = await fetch(`/api/due-date-reminders?month=${month}`);
            const result = await response.json();

            if (result.success && result.data && result.data.length > 0) {
              // Clear existing content
              contentContainer.innerHTML = '';

              // Create content items
              result.data.forEach(item => {
                // Date element
                const dateDiv = document.createElement('div');
                dateDiv.className = 'news mb-3 fw-semibold';
                dateDiv.textContent = item.date;

                // Content element
                const contentP = document.createElement('p');
                contentP.className = 'news fs-6 lh-base';
                // For BLOGS, render HTML content directly so that any <a> tags
                // inside the content are visible and clickable in the widget box.
                // This assumes the HTML stored for blogs is trusted.
                contentP.innerHTML = item.content || '';

                contentContainer.appendChild(dateDiv);
                contentContainer.appendChild(contentP);
              });

              // Restart marquee animation
              restartMarquee();
            } else {
              // No data available
              contentContainer.innerHTML = '<div class="news mb-3 fw-semibold">No reminders available</div>';
            }

            // Highlight the selected month
            highlightMonth(month);
            currentMonth = month;
          } catch (error) {
            console.error('Error loading due date reminders:', error);
            contentContainer.innerHTML = '<div class="news mb-3 fw-semibold">Error loading reminders</div>';
          }
        }

        // Function to restart marquee animation
        function restartMarquee() {
          const content = contentContainer;
          const box = marqueeBox;

          if (!box || !content) return;

          const boxHeight = box.offsetHeight;
          const contentHeight = content.scrollHeight;

          if (contentHeight > boxHeight) {
            const duration = (contentHeight / 50) * 1000;
            content.style.transform = `translateY(${boxHeight}px)`;
            content.style.animation = `marquee ${duration}ms linear infinite`;
          } else {
            content.style.animation = 'none';
            content.style.transform = 'translateY(0)';
          }
        }

        // Add click handlers to month links
        monthLinks.forEach(link => {
          link.addEventListener('click', function(e) {
            e.preventDefault();
            const month = this.getAttribute('data-month');
            loadDueDateReminders(month);
          });
        });

        // Load all reminders on page load
        window.addEventListener('DOMContentLoaded', function() {
          loadDueDateReminders('all');
        });

        // Also load when page is fully loaded (in case DOMContentLoaded already fired)
        if (document.readyState === 'loading') {
          document.addEventListener('DOMContentLoaded', function() {
            loadDueDateReminders('all');
          });
        } else {
          loadDueDateReminders('all');
        }
      })();

      // NEWS functionality
      (function() {
        const contentContainer = document.getElementById('newsContent');
        const marqueeBox = document.getElementById('newsMarquee');

        // Function to remove HTML tags and get plain text
        function stripHtml(html) {
          const tmp = document.createElement('DIV');
          tmp.innerHTML = html;
          return tmp.textContent || tmp.innerText || '';
        }

        // Function to restart marquee animation
        function restartMarquee() {
          const content = contentContainer;
          const box = marqueeBox;

          if (!box || !content) return;

          const boxHeight = box.offsetHeight;
          const contentHeight = content.scrollHeight;

          if (contentHeight > boxHeight) {
            const duration = (contentHeight / 50) * 1000;
            content.style.transform = `translateY(${boxHeight}px)`;
            content.style.animation = `marquee ${duration}ms linear infinite`;
          } else {
            content.style.animation = 'none';
            content.style.transform = 'translateY(0)';
          }
        }

        // Function to load and display news
        async function loadNews() {
          try {
            const response = await fetch('/api/news');
            const result = await response.json();

            if (result.success && result.data && result.data.length > 0) {
              // Clear existing content
              contentContainer.innerHTML = '';

              // Create content items
              result.data.forEach(item => {
                // Date element
                const dateDiv = document.createElement('div');
                dateDiv.className = 'news mb-3 fw-semibold';
                dateDiv.textContent = item.date;

                // Content element
                const contentP = document.createElement('p');
                contentP.className = 'news fs-6 lh-base';
                // Strip HTML tags for display
                const plainContent = stripHtml(item.content);
                contentP.textContent = plainContent;

                // If there's a link URL, wrap content in a link
                if (item.linkUrl) {
                  const linkWrapper = document.createElement('a');
                  linkWrapper.href = item.linkUrl;
                  linkWrapper.target = '_blank';
                  linkWrapper.style.textDecoration = 'none';
                  linkWrapper.style.color = 'inherit';
                  linkWrapper.appendChild(contentP);
                  contentContainer.appendChild(dateDiv);
                  contentContainer.appendChild(linkWrapper);
                } else {
                  contentContainer.appendChild(dateDiv);
                  contentContainer.appendChild(contentP);
                }
              });

              // Restart marquee animation
              restartMarquee();
            } else {
              // No data available
              contentContainer.innerHTML = '<div class="news mb-3 fw-semibold">No news available</div>';
            }
          } catch (error) {
            console.error('Error loading news:', error);
            contentContainer.innerHTML = '<div class="news mb-3 fw-semibold">Error loading news</div>';
          }
        }

        // Load news on page load
        if (document.readyState === 'loading') {
          document.addEventListener('DOMContentLoaded', loadNews);
        } else {
          loadNews();
        }
      })();

      // BLOGS functionality
      (function() {
        const contentContainer = document.getElementById('blogsContent');
        const marqueeBox = document.getElementById('blogsMarquee');

        // Function to restart marquee animation
        function restartMarquee() {
          const content = contentContainer;
          const box = marqueeBox;

          if (!box || !content) return;

          const boxHeight = box.offsetHeight;
          const contentHeight = content.scrollHeight;

          if (contentHeight > boxHeight) {
            const duration = (contentHeight / 50) * 1000;
            content.style.transform = `translateY(${boxHeight}px)`;
            content.style.animation = `marquee ${duration}ms linear infinite`;
          } else {
            content.style.animation = 'none';
            content.style.transform = 'translateY(0)';
          }
        }

        // Function to load and display blogs
        async function loadBlogs() {
          try {
            const response = await fetch('/api/blogs');
            const result = await response.json();

            if (result.success && result.data && result.data.length > 0) {
              // Clear existing content
              contentContainer.innerHTML = '';

              // Create content items
              result.data.forEach(item => {
                // Date element
                const dateDiv = document.createElement('div');
                dateDiv.className = 'news mb-3 fw-semibold';
                dateDiv.textContent = item.date;

                // Content element wrapper (use div so any <p> inside content is valid)
                const contentDiv = document.createElement('div');
                contentDiv.className = 'news fs-6 lh-base';
                // For BLOGS, render HTML content directly so any <a> tags in the
                // content are visible and clickable in the widget box.
                // Assumes the BLOGS HTML stored in DB is trusted.
                contentDiv.innerHTML = item.content || '';

                contentContainer.appendChild(dateDiv);
                contentContainer.appendChild(contentDiv);
              });

              // Restart marquee animation
              restartMarquee();
            } else {
              // No data available
              contentContainer.innerHTML = '<div class="news mb-3 fw-semibold">No blogs available</div>';
            }
          } catch (error) {
            console.error('Error loading blogs:', error);
            contentContainer.innerHTML = '<div class="news mb-3 fw-semibold">Error loading blogs</div>';
          }
        }

        // Load blogs on page load
        if (document.readyState === 'loading') {
          document.addEventListener('DOMContentLoaded', loadBlogs);
        } else {
          loadBlogs();
        }
      })();
    </script>
    
