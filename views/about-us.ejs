    <!-- Initialize Owl Carousel for Services -->
    <script>
      $(document).ready(function(){
        var servicesCarousel = $("#servicesCarousel").owlCarousel({
          loop: true,
          margin: 30,
          nav: true,
          dots: false,
          autoplay: true,
          autoplayTimeout: 3000,
          autoplayHoverPause: true,
          responsive: {
            0: {
              items: 1
            },
            600: {
              items: 2
            },
            1000: {
              items: 3
            }
          },
          navText: [
            '<i class="fa-solid fa-chevron-left"></i>',
            '<i class="fa-solid fa-chevron-right"></i>'
          ],
          onInitialized: function() {
            setTimeout(equalizeCardHeights, 100);
          },
          onTranslated: function() {
            setTimeout(equalizeCardHeights, 100);
          }
        });

        // Function to equalize card heights within visible items
        function equalizeCardHeights() {
          var $carousel = $("#servicesCarousel");
          var $visibleItems = $carousel.find(".owl-item.active:not(.cloned), .owl-item.active:not(.cloned)");
          
          // Reset heights first
          $carousel.find(".owl-item .item .card").css('min-height', 'auto');
          
          // Process each group of visible items (based on items shown)
          var itemsToShow = getCurrentItemsCount();
          
          $visibleItems.each(function(index) {
            if (index % itemsToShow === 0) {
              var $group = $visibleItems.slice(index, index + itemsToShow);
              var maxHeight = 0;
              
              // Find max height in this group
              $group.find(".item .card").each(function() {
                var height = $(this).outerHeight();
                if (height > maxHeight) {
                  maxHeight = height;
                }
              });
              
              // Set all cards in group to max height
              $group.find(".item .card").css('min-height', maxHeight + 'px');
            }
          });
        }
        
        // Get current number of items to show based on screen size
        function getCurrentItemsCount() {
          var width = $(window).width();
          if (width >= 1000) return 3;
          if (width >= 600) return 2;
          return 1;
        }

        // Equalize heights on window resize
        var resizeTimer;
        $(window).on('resize', function() {
          clearTimeout(resizeTimer);
          resizeTimer = setTimeout(function() {
            equalizeCardHeights();
          }, 250);
        });

        // Initial equalization after page load
        setTimeout(function() {
          equalizeCardHeights();
        }, 600);
      });
    </script>

